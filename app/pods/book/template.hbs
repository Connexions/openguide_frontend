{{outlet}}
<div class="container main">
<div class="row">
  <div class="col-lg-12 title">
  <h1>{{model.title}}</h1>
  </div>
</div>
<div class="row">
{{#each model.elements as |element|}}
  <div class="col-lg-4">
    <div class="box">
  <h2>{{#link-to 'element' element}}{{element.name}}{{/link-to}}</h2>
  </div>
  </div>
  {{else}}
  <div class="col-lg-12">
    <p>Sorry, nothing here...</p>
  </div>
{{/each}}
</div>
</div>

