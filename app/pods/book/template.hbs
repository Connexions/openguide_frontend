{{outlet}}
<h1>{{model.title}} <small> / {{#link-to 'theme' model.theme}}{{model.theme.title}}{{/link-to}}</small></h1>
{{#each element in model.elements}}
<div class="row">
  <div class="box">
    <div class="row">
    <h3 class="title">{{element.name}} - {{element.bookPart}}</h3>
    </div>
    <div class="row">
      {{#each attribute in element.elementAttributes}}
      <div class="thumb"> 
        {{#unless attribute.text}}
        <h4>{{attribute.label}}</h4>
        <a href="{{attribute.image}}" target="_blank"><img src="{{attribute.thumb}}" alt=""/></a>
        {{/unless}}
      </div>
      {{/each}}
    </div>
    <hr/>
    <div class="row">
    <div class="attrs">
            {{#each attribute in element.elementAttributes}}

      {{#if attribute.text}}
      <p><strong>{{attribute.label}}</strong>: {{attribute.text}}</p>
      {{/if}}
      {{/each}}
    </div>
    </div>
  </div>
</div>  
{{/each}}